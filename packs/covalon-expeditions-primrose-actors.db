ure</span></strong><span style=\"font-family: var(--font-primary); font-size: var(--font-size-14);\"> The creature is pushed 20 feet away from Saddam and falls prone.</span></p>"},"source":{"value":""},"traits":{"value":["air","arcane","evocation"],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-16T19:30:57.816+01:00","version":{"schema":0.764,"foundry":"9.269","system":"3.13.5.16"}}},"actionType":{"value":"reaction"},"actionCategory":{"value":""},"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false,"rarity":{"value":"common"},"weapon":{"value":""},"skill_requirements":{"skill":"","rank":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"7RtLbrlfn9D7fL4K":3},"flags":{}},{"_id":"GdETVstixMk4QoXc","name":"Arcane Innate Spells","type":"spellcastingEntry","img":"systems/pf2e/icons/default-icons/spellcastingEntry.svg","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-16T19:30:57.816+01:00","version":{"schema":0.764,"foundry":"9.269","system":"3.13.5.16"}}},"ability":{"value":"cha"},"spelldc":{"value":26,"dc":34,"mod":0,"item":0},"tradition":{"value":"arcane"},"prepared":{"value":"innate","flexible":false},"showUnpreparedSpells":{"value":true},"showSlotlessLevels":{"value":true},"proficiency":{"value":1},"displayLevels":{},"slots":{"slot0":{"prepared":[],"value":0,"max":0},"slot1":{"prepared":[],"value":0,"max":0},"slot2":{"prepared":[],"value":0,"max":0},"slot3":{"prepared":[],"value":0,"max":0},"slot4":{"prepared":[],"value":0,"max":0},"slot5":{"prepared":[],"value":0,"max":0},"slot6":{"prepared":[],"value":0,"max":0},"slot7":{"prepared":[],"value":0,"max":0},"slot8":{"prepared":[],"value":0,"max":0},"slot9":{"prepared":[],"value":0,"max":0},"slot10":{"prepared":[],"value":0,"max":0},"slot11":{"prepared